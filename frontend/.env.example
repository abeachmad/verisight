# Backend API
REACT_APP_BACKEND_URL=http://localhost:8001

# Linera Testnet (Public URLs only - no sensitive APP_IDs)
REACT_APP_LINERA_TESTNET_SERVICE_URL=https://rpc.testnet.linera.net
REACT_APP_IPFS_GATEWAY_URL=https://ipfs.io/ipfs

# WalletConnect
REACT_APP_WALLETCONNECT_PROJECT_ID=your-walletconnect-project-id

# Development
WDS_SOCKET_PORT=443
REACT_APP_ENABLE_VISUAL_EDITS=true
ENABLE_HEALTH_CHECK=false

# SECURITY NOTE:
# - Only expose public URLs in REACT_APP_* variables
# - Never expose APP_IDs, private keys, or secrets in frontend env
# - APP_IDs are fetched from backend API at runtime
