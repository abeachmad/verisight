# Linera Mode Configuration Template
# Copy values to frontend/.env.local after deployment

# Mode
REACT_APP_MODE=LINERA
REACT_APP_LINERA_ENABLED=true

# Backend
REACT_APP_BACKEND_URL=http://127.0.0.1:8001

# Linera Service (GraphQL endpoint)
REACT_APP_LINERA_TESTNET_SERVICE_URL=http://127.0.0.1:8080/graphql

# Application IDs (from deployment)
REACT_APP_ORACLE_FEED_APP_ID=<ORACLE_FEED_APP_ID>
REACT_APP_MARKET_APP_ID=<MARKET_APP_ID>

# Chain ID (optional)
REACT_APP_CHAIN_ID=<CHAIN_ID>

# IPFS Gateway
REACT_APP_IPFS_GATEWAY_URL=https://ipfs.io/ipfs
